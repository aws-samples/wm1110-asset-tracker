&qspi {
	status = "okay";
	pinctrl-0 = <&qspi_default>;
	pinctrl-1 = <&qspi_sleep>;
	pinctrl-names = "default", "sleep";
	p25q32sh: p25q32sh@0 {
		compatible = "nordic,qspi-nor";
		reg = <0>;
		sck-frequency = <100000000>;
		quad-enable-requirements = "S2B1v1";
		jedec-id = [85 60 16];
		sfdp-bfp = [
			e5 20 f1 ff  ff ff ff 00  44 eb 08 6b  08 3b 80 bb
			ee ff ff ff  ff ff 00 ff  ff ff 00 ff  0c 20 0f 52
			10 d8 08 81
		];
		size = <33554432>;
		has-dpd;
		t-enter-dpd = <3000>;
		t-exit-dpd = <8000>;
	};
};

&ieee802154 {
	status = "okay";
};

&flash0 {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		boot_mbr_partition: partition@0 {
			label = "boot_mbr";
			reg = <0x00000000 0x00001000>;
		};

		slot0_partition: partition@1000 {
			label = "code";
			reg = <0x00001000 0x000cf000>;
		};

		mfg_storage: partition@d0000 {
			label = "mfg_storage";
			reg = <0x000d0000 0x00001000>;
		};

		sidewalk_storage: partition@d1000 {
			label = "sidewalk_storage";
			reg = <0x000d1000 0x00001000>;
		};

		storage_partition: partition@d2000 {
			label = "storage";
			reg = <0x000d2000 0x00010000>;
		};

		hw_unique_key_partition: partition@f3000 {
			label = "hw_unique_key";
			reg = <0x000f3000 0x00001000>;
		};

		bootloader_partition: partition@f4000 {
			label = "bootloader";
			reg = <0x000f4000 0x00008000>;
		};

		bootloader_mbr_params_partition: partition@fe000 {
			label = "bootloader_mbr_params";
			reg = <0x000fe000 0x00001000>;
		};

		bootloader_settings_partition: partition@ff000 {
			label = "bootloader_settings";
			reg = <0x000ff000 0x00001000>;
		};
	};
};

zephyr_udc0: &usbd {
	compatible = "nordic,nrf-usbd";
	status = "okay";

	usb_cdc_acm_uart: cdc-acm-uart {
		compatible = "zephyr,cdc-acm-uart";
	};
};
