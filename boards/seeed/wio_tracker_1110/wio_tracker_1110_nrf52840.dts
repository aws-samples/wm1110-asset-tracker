/dts-v1/;
#include <nordic/nrf52840_qiaa.dtsi>
#include <zephyr/dt-bindings/gpio/gpio.h>
#include <zephyr/dt-bindings/led/led.h>
#include "wio_tracker_1110_nrf52840-pinctrl.dtsi"
#include <zephyr/dt-bindings/i2c/i2c.h>
#include <zephyr/dt-bindings/lora_lbm/lr11xx.h>

/ {
	model = "Seeed Wio Tracker 1110";
	compatible = "seeed,wio-tracker-1110";

	chosen {
		zephyr,console = &usb_cdc_acm_uart;
		zephyr,shell-uart = &usb_cdc_acm_uart;
		nordic,pm-ext-flash = &p25q32sh;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,code-partition = &slot0_partition;
		zephyr,ieee802154 = &ieee802154;
		zephyr,lora-transceiver = &lora_lr1110;
	};

	leds {
		compatible = "gpio-leds";
		led0: led_0 {
			gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
			label = "Blue LED";
		};
	};

	buttons {
		compatible = "gpio-keys";
		button0: button_0 {
			gpios = <&gpio1 2 (GPIO_PULL_UP | GPIO_ACTIVE_LOW)>;
			label = "Push button switch 0";
			zephyr,code = <11>;
		};
	};

	grove_digital0: grove_digital0 {
		compatible = "grove-header";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpio0 13 0>,
				<1 0 &gpio0 14 0>;
	};

	grove_digital1: grove_digital1 {
		compatible = "grove-header";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpio0 15 0>,
				<1 0 &gpio0 16 0>;
	};

	grove_digital2: grove_digital2 {
		compatible = "grove-header";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpio0 30 0>,
				<1 0 &gpio0 31 0>;
	};

	grove_digital_analog: grove_digital_analog {
		compatible = "grove-header";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpio0 28 0>,
				<1 0 &gpio0 29 0>;
	};

	grove_digital_uart: grove_digital_uart {
		compatible = "grove-header";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpio0 24 0>,
				<1 0 &gpio0 25 0>;
	};

	grove_digital_i2c: grove_digital_i2c {
		compatible = "grove-header";
		#gpio-cells = <2>;
		gpio-map-mask = <0xffffffff 0xffffffc0>;
		gpio-map-pass-thru = <0 0x3f>;
		gpio-map = <0 0 &gpio0 4 0>,
				<1 0 &gpio0 5 0>;
	};

	/* Regulators */
	B3V3_pwr: grove_power_3v3_en {
		compatible = "regulator-fixed";
		regulator-name = "grove_headers_power_3v3_enable";
		enable-gpios = <&gpio1 1 GPIO_ACTIVE_HIGH>;
		regulator-boot-on;
		startup-delay-us = <20000>;
	};

	/* These aliases are provided for compatibility with samples */
	aliases {
		led0 = &led0;
		sw0 = &button0;
		button0 = &button0;
		watchdog0 = &wdt0;
		spi-flash0 = &p25q32sh;
		accel0 = &lis3dh;
		lora-transceiver = &lora_lr1110;
	};
};

&adc {
	status = "okay";
};

&gpiote {
	status = "okay";
};

&gpio0 {
	status = "okay";
};

&gpio1 {
	status = "okay";
};

grove_uart: &uart0 {
	compatible = "nordic,nrf-uarte";
	status = "okay";
	current-speed = <115200>;
	pinctrl-0 = <&uart0_default>;
	pinctrl-1 = <&uart0_sleep>;
	pinctrl-names = "default", "sleep";
};

grove_i2c: &i2c1 {
	compatible = "nordic,nrf-twim";
	status = "okay";
	pinctrl-0 = <&i2c1_default>;
	pinctrl-1 = <&i2c1_sleep>;
	pinctrl-names = "default", "sleep";
	clock-frequency = <I2C_BITRATE_FAST>;

	sht41: sht4x@44 {
		compatible = "sensirion,sht4x";
		reg = <0x44>;
		repeatability = <2>;
		vin-supply = <&B3V3_pwr>;
	};

	lis3dh: lis3dh@19 {
		compatible = "st,lis3dh", "st,lis2dh";
		reg = <0x19>;
		irq-gpios = <&gpio1 3 GPIO_ACTIVE_HIGH>,
				<&gpio1 4 GPIO_ACTIVE_HIGH>;
		vin-supply = <&B3V3_pwr>;
	};
};

&pwm0 {
	status = "okay";
	pinctrl-0 = <&pwm0_default>;
	pinctrl-1 = <&pwm0_sleep>;
	pinctrl-names = "default", "sleep";
};

sid_semtech: &spi0 {
	compatible = "nordic,nrf-spi";
	status = "okay";
	pinctrl-0 = <&spi0_default>;
	pinctrl-1 = <&spi0_sleep>;
	pinctrl-names = "default", "sleep";
	clock-frequency = <DT_FREQ_M(8)>;
	cs-gpios = <&gpio1 12 GPIO_ACTIVE_LOW>;

	lora_semtech_lr11xxmb1xxs: lora_lr1110: lora@0 {
		compatible = "semtech,lr1110";
		reg = <0>;
		spi-max-frequency = <DT_FREQ_M(8)>;

		reset-gpios = <&gpio1 10 GPIO_ACTIVE_LOW>;
		busy-gpios = <&gpio1 11 GPIO_ACTIVE_HIGH>;
		event-gpios = <&gpio0 2 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>;
		gnss-lna-gpios = <&gpio1 5 GPIO_ACTIVE_HIGH>;

		lf-tx-path = <LR11XX_TX_PATH_LF_LP_HP>;
		lf-clk = <LR11XX_LFCLK_XTAL>;
		reg-mode = <LR11XX_REG_MODE_DCDC>;

		rf-sw-enable = <(LR11XX_DIO5 | LR11XX_DIO6 | LR11XX_DIO7)>;
		rf-sw-rx-mode = <LR11XX_DIO5>;
		rf-sw-tx-mode = <(LR11XX_DIO5 | LR11XX_DIO6)>;
		rf-sw-tx-hp-mode = <LR11XX_DIO6>;
		rf-sw-gnss-mode = <LR11XX_DIO7>;

		tcxo-voltage = <LR11XX_TCXO_SUPPLY_1_8V>;
		tcxo-wakeup-time = <5>;

		tx-power-cfg-lf-lp =
			< (-15) 0x00 0x00>,
			< (-14) 0x00 0x00>,
